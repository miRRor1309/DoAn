@model IEnumerable<DoAn.Models.UserInfo>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.SDT)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AdminOrCus)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TenTK)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PassWord)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.SDT)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.AdminOrCus)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TenTK)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PassWord)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Email)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.maUser }) |
                    @Html.ActionLink("Details", "Details", new { id = item.maUser }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.maUser })
                </td>
            </tr>
        }

    </table>
